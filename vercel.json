{
    "version": 2,
    "builds": [
      {
        "src": "src/api/weather.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/weather",
        "dest": "/src/api/weather.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.html"
      }
    ]
  }